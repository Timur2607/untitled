–ó–∞–¥–∞—á–∞ 1

class Person {
  String name;  // –ò–º—è —á–µ–ª–æ–≤–µ–∫–∞
  int age;      // –í–æ–∑—Ä–∞—Å—Ç —á–µ–ª–æ–≤–µ–∫–∞
  String gender; // –ü–æ–ª —á–µ–ª–æ–≤–µ–∫–∞

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞ Person
  Person({required this.name, required this.age, required this.gender});

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≤—ã–≤–æ–¥–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —á–µ–ª–æ–≤–µ–∫–µ
  void displayInfo() {
    print('–ò–º—è: $name');
    print('–í–æ–∑—Ä–∞—Å—Ç: $age');
    print('–ü–æ–ª: $gender');
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –≤–æ–∑—Ä–∞—Å—Ç–∞ –Ω–∞ 1 –≥–æ–¥
  void incrementAge() {
    age++;
    print('–í–æ–∑—Ä–∞—Å—Ç —É–≤–µ–ª–∏—á–µ–Ω. –ù–æ–≤—ã–π –≤–æ–∑—Ä–∞—Å—Ç: $age');
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è –∏–º–µ–Ω–∏
  void changeName(String newName) {
    name = newName;
    print('–ò–º—è –∏–∑–º–µ–Ω–µ–Ω–æ –Ω–∞: $name');
  }
}

void main() {
  // –°–æ–∑–¥–∞–Ω–∏–µ –æ–±—ä–µ–∫—Ç–∞ –∫–ª–∞—Å—Å–∞ Person
  Person person = Person(name: '–ê–ª–µ–∫—Å–µ–π', age: 25, gender: '–ú—É–∂—Å–∫–æ–π');

  // –í—ã–≤–æ–¥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —á–µ–ª–æ–≤–µ–∫–µ
  person.displayInfo();

  // –£–≤–µ–ª–∏—á–µ–Ω–∏–µ –≤–æ–∑—Ä–∞—Å—Ç–∞
  person.incrementAge();

  // –ò–∑–º–µ–Ω–µ–Ω–∏–µ –∏–º–µ–Ω–∏
  person.changeName('–ò–≤–∞–Ω');

  // –í—ã–≤–æ–¥ –æ–±–Ω–æ–≤–ª–µ–Ω–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
  person.displayInfo();
}

–ó–∞–¥–∞—á–∞ 2
class Person {
  String name;  // –ò–º—è —á–µ–ª–æ–≤–µ–∫–∞
  int age;      // –í–æ–∑—Ä–∞—Å—Ç
  String gender; // –ü–æ–ª

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –∫–ª–∞—Å—Å–∞ Person
  Person({required this.name, required this.age, required this.gender});

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≤—ã–≤–æ–¥–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —á–µ–ª–æ–≤–µ–∫–µ
  void displayInfo() {
    print('–ò–º—è: $name');
    print('–í–æ–∑—Ä–∞—Å—Ç: $age');
    print('–ü–æ–ª: $gender');
  }
}

// –ö–ª–∞—Å—Å Worker (–Ω–∞—Å–ª–µ–¥—É–µ—Ç—Å—è –æ—Ç Person)
class Worker extends Person {
  double salary; // –ü–æ–ª–µ –∑–∞—Ä–ø–ª–∞—Ç—ã

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –∫–ª–∞—Å—Å–∞ Worker
  Worker({required String name, required int age, required String gender, required this.salary})
      : super(name: name, age: age, gender: gender);

  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ displayInfo
  @override
  void displayInfo() {
    super.displayInfo(); // –í—ã–∑–æ–≤ –º–µ—Ç–æ–¥–∞ displayInfo –∫–ª–∞—Å—Å–∞-—Ä–æ–¥–∏—Ç–µ–ª—è
    print('–ó–∞—Ä–ø–ª–∞—Ç–∞: $salary');
  }
}

// –ö–ª–∞—Å—Å Manager (–Ω–∞—Å–ª–µ–¥—É–µ—Ç—Å—è –æ—Ç Worker)
class Manager extends Worker {
  List<Worker> subordinates; // –°–ø–∏—Å–æ–∫ –ø–æ–¥—á–∏–Ω–µ–Ω–Ω—ã—Ö —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–æ–≤

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –∫–ª–∞—Å—Å–∞ Manager
  Manager({
    required String name,
    required int age,
    required String gender,
    required double salary,
    required this.subordinates,
  }) : super(name: name, age: age, gender: gender, salary: salary);

  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ displayInfo
  @override
  void displayInfo() {
    super.displayInfo(); // –í—ã–∑–æ–≤ –º–µ—Ç–æ–¥–∞ displayInfo –∫–ª–∞—Å—Å–∞ Worker
    print('–ü–æ–¥—á–∏–Ω–µ–Ω–Ω—ã–µ:');
    if (subordinates.isEmpty) {
      print('–ù–µ—Ç –ø–æ–¥—á–∏–Ω–µ–Ω–Ω—ã—Ö.');
    } else {
      for (var worker in subordinates) {
        print('- ${worker.name}');
      }
    }
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø–æ–¥—á–∏–Ω–µ–Ω–Ω–æ–≥–æ
  void addSubordinate(Worker worker) {
    subordinates.add(worker);
    print('–ü–æ–¥—á–∏–Ω–µ–Ω–Ω—ã–π ${worker.name} –¥–æ–±–∞–≤–ª–µ–Ω.');
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è –ø–æ–¥—á–∏–Ω–µ–Ω–Ω–æ–≥–æ
  void removeSubordinate(Worker worker) {
    subordinates.remove(worker);
    print('–ü–æ–¥—á–∏–Ω–µ–Ω–Ω—ã–π ${worker.name} —É–¥–∞–ª–µ–Ω.');
  }
}

// –ü—Ä–∏–º–µ—Ä —Ä–∞–±–æ—Ç—ã
void main() {
  // –°–æ–∑–¥–∞–Ω–∏–µ —Ä–∞–±–æ—Ç–Ω–∏–∫–∞
  Worker worker1 = Worker(name: '–ò–≤–∞–Ω', age: 30, gender: '–ú—É–∂—Å–∫–æ–π', salary: 50000);
  Worker worker2 = Worker(name: '–û–ª—å–≥–∞', age: 27, gender: '–ñ–µ–Ω—Å–∫–∏–π', salary: 48000);

  // –°–æ–∑–¥–∞–Ω–∏–µ –º–µ–Ω–µ–¥–∂–µ—Ä–∞
  Manager manager = Manager(
    name: '–ê–ª–µ–∫—Å–∞–Ω–¥—Ä',
    age: 40,
    gender: '–ú—É–∂—Å–∫–æ–π',
    salary: 100000,
    subordinates: [worker1], // –û–¥–∏–Ω –ø–æ–¥—á–∏–Ω–µ–Ω–Ω—ã–π –∏–∑–Ω–∞—á–∞–ª—å–Ω–æ
  );

  // –í—ã–≤–æ–¥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –º–µ–Ω–µ–¥–∂–µ—Ä–µ
  manager.displayInfo();

  // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –Ω–æ–≤–æ–≥–æ –ø–æ–¥—á–∏–Ω–µ–Ω–Ω–æ–≥–æ
  manager.addSubordinate(worker2);

  // –ü–æ–≤—Ç–æ—Ä–Ω—ã–π –≤—ã–≤–æ–¥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –º–µ–Ω–µ–¥–∂–µ—Ä–µ
  manager.displayInfo();

  // –£–¥–∞–ª–µ–Ω–∏–µ –ø–æ–¥—á–∏–Ω–µ–Ω–Ω–æ–≥–æ
  manager.removeSubordinate(worker1);

  // –û—á–µ—Ä–µ–¥–Ω–æ–π –≤—ã–≤–æ–¥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
  manager.displayInfo();
}


–ó–∞–¥–∞—á–∞ 3

// –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å Animal
abstract class Animal {
  void makeSound(); // –ê–±—Å—Ç—Ä–∞–∫—Ç–Ω—ã–π –º–µ—Ç–æ–¥ –¥–ª—è –≤–æ—Å–ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è –∑–≤—É–∫–∞ –∂–∏–≤–æ—Ç–Ω–æ–≥–æ
}

// –ö–ª–∞—Å—Å Dog, —Ä–µ–∞–ª–∏–∑—É—é—â–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å Animal
class Dog implements Animal {
  @override
  void makeSound() {
    print('–°–æ–±–∞–∫–∞: –ì–∞–≤-–≥–∞–≤!');
  }
}

// –ö–ª–∞—Å—Å Cat, —Ä–µ–∞–ª–∏–∑—É—é—â–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å Animal
class Cat implements Animal {
  @override
  void makeSound() {
    print('–ö–æ—à–∫–∞: –ú—è—É-–º—è—É!');
  }
}

// –ö–ª–∞—Å—Å Cow, —Ä–µ–∞–ª–∏–∑—É—é—â–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å Animal
class Cow implements Animal {
  @override
  void makeSound() {
    print('–ö–æ—Ä–æ–≤–∞: –ú—É-–º—É!');
  }
}

void main() {
  // –°–æ–∑–¥–∞–Ω–∏–µ –º–∞—Å—Å–∏–≤–∞ –∂–∏–≤–æ—Ç–Ω—ã—Ö
  List<Animal> animals = [
    Dog(),
    Cat(),
    Cow(),
  ];

  // –î–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏—è –ø–æ–ª–∏–º–æ—Ä—Ñ–∏–∑–º–∞
  for (Animal animal in animals) {
    animal.makeSound(); // –í—ã–∑–æ–≤ –º–µ—Ç–æ–¥–∞ makeSound() –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –∂–∏–≤–æ—Ç–Ω–æ–≥–æ
  }
}

–ó–∞–¥–∞—á–∞ 4 

// –ê–±—Å—Ç—Ä–∞–∫—Ç–Ω—ã–π –∫–ª–∞—Å—Å Transport
abstract class Transport {
  // –ê–±—Å—Ç—Ä–∞–∫—Ç–Ω—ã–π –º–µ—Ç–æ–¥ move()
  void move();
}

// –ö–ª–∞—Å—Å Car, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π Transport
class Car extends Transport {
  @override
  void move() {
    print('–ú–∞—à–∏–Ω–∞ –µ–¥–µ—Ç –ø–æ –¥–æ—Ä–æ–≥–µ.');
  }
}

// –ö–ª–∞—Å—Å Bike, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π Transport
class Bike extends Transport {
  @override
  void move() {
    print('–í–µ–ª–æ—Å–∏–ø–µ–¥ –¥–≤–∏–∂–µ—Ç—Å—è –ø–æ –≤–µ–ª–æ–¥–æ—Ä–æ–∂–∫–µ.');
  }
}

void main() {
  // –ü–æ–ª–∏–º–æ—Ä—Ñ–∏–∑–º: –°–ø–∏—Å–æ–∫ —Ä–∞–∑–Ω—ã—Ö –≤–∏–¥–æ–≤ —Ç—Ä–∞–Ω—Å–ø–æ—Ä—Ç–∞
  List<Transport> transports = [
    Car(),
    Bike(),
  ];

  // –î–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏—è –≤—ã–∑–æ–≤–∞ –º–µ—Ç–æ–¥–∞ move() –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –æ–±—ä–µ–∫—Ç–∞
  for (Transport transport in transports) {
    transport.move();
  }
}


–ó–∞–¥–∞—á–∞ 5 
// –ö–ª–∞—Å—Å Book
class Book {
  // –ü–æ–ª—è –∫–Ω–∏–≥–∏
  String title;
  String author;
  int year;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  Book(this.title, this.author, this.year);

  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ toString –¥–ª—è —É–¥–æ–±—Å—Ç–≤–∞ –≤—ã–≤–æ–¥–∞ –Ω–∞ —ç–∫—Ä–∞–Ω
  @override
  String toString() {
    return '–ö–Ω–∏–≥–∞: "$title", –ê–≤—Ç–æ—Ä: $author, –ì–æ–¥: $year';
  }
}

// –ö–ª–∞—Å—Å Library
class Library {
  // –ö–æ–ª–ª–µ–∫—Ü–∏—è –∫–Ω–∏–≥
  List<Book> books = [];

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –∫–Ω–∏–≥–∏
  void addBook(Book book) {
    books.add(book);
    print('–ö–Ω–∏–≥–∞ "${book.title}" –¥–æ–±–∞–≤–ª–µ–Ω–∞ –≤ –±–∏–±–ª–∏–æ—Ç–µ–∫—É.');
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –ø–æ–∏—Å–∫–∞ –∫–Ω–∏–≥ –ø–æ –∞–≤—Ç–æ—Ä—É
  List<Book> findBooksByAuthor(String author) {
    return books.where((book) => book.author.toLowerCase() == author.toLowerCase()).toList();
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –ø–æ–∏—Å–∫–∞ –∫–Ω–∏–≥ –ø–æ –≥–æ–¥—É –ø—É–±–ª–∏–∫–∞—Ü–∏–∏
  List<Book> findBooksByYear(int year) {
    return books.where((book) => book.year == year).toList();
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –≤—Å–µ—Ö –∫–Ω–∏–≥ –≤ –±–∏–±–ª–∏–æ—Ç–µ–∫–µ
  void displayBooks() {
    if (books.isEmpty) {
      print('–ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ –ø—É—Å—Ç–∞.');
    } else {
      print('–ö–Ω–∏–≥–∏ –≤ –±–∏–±–ª–∏–æ—Ç–µ–∫–µ:');
      books.forEach((book) => print(book));
    }
  }
}

void main() {
  // –°–æ–∑–¥–∞–µ–º —ç–∫–∑–µ–º–ø–ª—è—Ä—ã –∫–Ω–∏–≥
  Book book1 = Book('–í–æ–π–Ω–∞ –∏ –º–∏—Ä', '–õ–µ–≤ –¢–æ–ª—Å—Ç–æ–π', 1869);
  Book book2 = Book('–ü—Ä–µ—Å—Ç—É–ø–ª–µ–Ω–∏–µ –∏ –Ω–∞–∫–∞–∑–∞–Ω–∏–µ', '–§–µ–¥–æ—Ä –î–æ—Å—Ç–æ–µ–≤—Å–∫–∏–π', 1866);
  Book book3 = Book('–ú–∞—Å—Ç–µ—Ä –∏ –ú–∞—Ä–≥–∞—Ä–∏—Ç–∞', '–ú–∏—Ö–∞–∏–ª –ë—É–ª–≥–∞–∫–æ–≤', 1967);

  // –°–æ–∑–¥–∞–µ–º —ç–∫–∑–µ–º–ø–ª—è—Ä –±–∏–±–ª–∏–æ—Ç–µ–∫–∏
  Library library = Library();

  // –î–æ–±–∞–≤–ª—è–µ–º –∫–Ω–∏–≥–∏ –≤ –±–∏–±–ª–∏–æ—Ç–µ–∫—É
  library.addBook(book1);
  library.addBook(book2);
  library.addBook(book3);

  // –û—Ç–æ–±—Ä–∞–∂–∞–µ–º –≤—Å–µ –∫–Ω–∏–≥–∏
  library.displayBooks();

  // –ò—â–µ–º –∫–Ω–∏–≥–∏ –ø–æ –∞–≤—Ç–æ—Ä—É
  print('\n–ü–æ–∏—Å–∫ –ø–æ –∞–≤—Ç–æ—Ä—É "–§–µ–¥–æ—Ä –î–æ—Å—Ç–æ–µ–≤—Å–∫–∏–π":');
  List<Book> foundByAuthor = library.findBooksByAuthor('–§–µ–¥–æ—Ä –î–æ—Å—Ç–æ–µ–≤—Å–∫–∏–π');
  foundByAuthor.forEach((book) => print(book));

  // –ò—â–µ–º –∫–Ω–∏–≥–∏ –ø–æ –≥–æ–¥—É –ø—É–±–ª–∏–∫–∞—Ü–∏–∏
  print('\n–ü–æ–∏—Å–∫ –ø–æ –≥–æ–¥—É 1967:');
  List<Book> foundByYear = library.findBooksByYear(1967);
  foundByYear.forEach((book) => print(book));
}

–ó–∞–¥–∞—á–∞ 6
class BankAccount {
  // –ü—Ä–∏–≤–∞—Ç–Ω—ã–µ –ø–æ–ª—è
  String _accountNumber; // –ù–æ–º–µ—Ä —Å—á–µ—Ç–∞ (–¥–æ—Å—Ç—É–ø–µ–Ω —Ç–æ–ª—å–∫–æ –≤–Ω—É—Ç—Ä–∏ –∫–ª–∞—Å—Å–∞)
  double _balance; // –ë–∞–ª–∞–Ω—Å (–¥–æ—Å—Ç—É–ø–µ–Ω —Ç–æ–ª—å–∫–æ –≤–Ω—É—Ç—Ä–∏ –∫–ª–∞—Å—Å–∞)

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  BankAccount(this._accountNumber, this._balance);

  // –ì–µ—Ç—Ç–µ—Ä –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–æ–º–µ—Ä–∞ —Å—á—ë—Ç–∞ (—Ç–æ–ª—å–∫–æ —á—Ç–µ–Ω–∏–µ)
  String get accountNumber => _accountNumber;

  // –ì–µ—Ç—Ç–µ—Ä –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –±–∞–ª–∞–Ω—Å–∞ (—Ç–æ–ª—å–∫–æ —á—Ç–µ–Ω–∏–µ)
  double get balance => _balance;

  // –ú–µ—Ç–æ–¥ –¥–ª—è –ø–æ–ø–æ–ª–Ω–µ–Ω–∏—è —Å—á—ë—Ç–∞
  void deposit(double amount) {
    if (amount <= 0) {
      print('–°—É–º–º–∞ –¥–ª—è –ø–æ–ø–æ–ª–Ω–µ–Ω–∏—è –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –±–æ–ª—å—à–µ 0.');
    } else {
      _balance += amount;
      print('–°—á—ë—Ç –ø–æ–ø–æ–ª–Ω–µ–Ω –Ω–∞ $amount. –¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: $_balance.');
    }
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —Å–Ω—è—Ç–∏—è —Å—Ä–µ–¥—Å—Ç–≤
  void withdraw(double amount) {
    if (amount <= 0) {
      print('–°—É–º–º–∞ –¥–ª—è —Å–Ω—è—Ç–∏—è –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –±–æ–ª—å—à–µ 0.');
    } else if (amount > _balance) {
      print('–ù–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Å—Ä–µ–¥—Å—Ç–≤! –î–æ—Å—Ç—É–ø–Ω—ã–π –±–∞–ª–∞–Ω—Å: $_balance.');
    } else {
      _balance -= amount;
      print('–°–Ω—è—Ç–æ $amount. –¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: $_balance.');
    }
  }
}

void main() {
  // –°–æ–∑–¥–∞–Ω–∏–µ –±–∞–Ω–∫–æ–≤—Å–∫–æ–≥–æ —Å—á—ë—Ç–∞
  BankAccount account = BankAccount('123456789', 1000.0);

  // –ü–æ–ø–æ–ª–Ω–µ–Ω–∏–µ —Å—á—ë—Ç–∞
  account.deposit(500); // –ü–æ–ø–æ–ª–Ω–µ–Ω–æ –Ω–∞ 500
  account.deposit(-100); // –û—à–∏–±–∫–∞

  // –°–Ω—è—Ç–∏–µ —Å—Ä–µ–¥—Å—Ç–≤
  account.withdraw(200); // –°–Ω—è—Ç–æ 200
  account.withdraw(1500); // –û—à–∏–±–∫–∞, –Ω–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Å—Ä–µ–¥—Å—Ç–≤
  account.withdraw(0); // –û—à–∏–±–∫–∞

  // –ü—Ä–æ—Å–º–æ—Ç—Ä —Ç–µ–∫—É—â–µ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è —Å—á–µ—Ç–∞
  print('–ù–æ–º–µ—Ä —Å—á—ë—Ç–∞: ${account.accountNumber}');
  print('–ë–∞–ª–∞–Ω—Å: ${account.balance}');
}

–ó–∞–¥–∞—á–∞ 7

class Counter {
  // –°—Ç–∞—Ç–∏—á–µ—Å–∫–æ–µ –ø–æ–ª–µ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è —á–∏—Å–ª–∞ —Å–æ–∑–¥–∞–Ω–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
  static int _count = 0;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  Counter() {
    _incrementCount();
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è —Å—á—ë—Ç—á–∏–∫–∞ –æ–±—ä–µ–∫—Ç–æ–≤
  static void _incrementCount() {
    _count++;
  }

  // –ì–µ—Ç—Ç–µ—Ä –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–µ–∫—É—â–µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏—è —Å—á—ë—Ç—á–∏–∫–∞
  static int get count => _count;
}

void main() {
  // –°–æ–∑–¥–∞—ë–º –Ω–µ—Å–∫–æ–ª—å–∫–æ –æ–±—ä–µ–∫—Ç–æ–≤ –∫–ª–∞—Å—Å–∞ Counter
  Counter obj1 = Counter();
  Counter obj2 = Counter();
  Counter obj3 = Counter();

  // –í—ã–≤–æ–¥–∏–º –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å–æ–∑–¥–∞–Ω–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
  print('–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å–æ–∑–¥–∞–Ω–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤: ${Counter.count}');
}


–ó–∞–¥–∞—á–∞ 8
// –ê–±—Å—Ç—Ä–∞–∫—Ç–Ω—ã–π –∫–ª–∞—Å—Å Shape
abstract class Shape {
  // –ê–±—Å—Ç—Ä–∞–∫—Ç–Ω—ã–π –º–µ—Ç–æ–¥ –¥–ª—è —Ä–∞—Å—á–µ—Ç–∞ –ø–ª–æ—â–∞–¥–∏
  double getArea();
}

// –ö–ª–∞—Å—Å Circle, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π –æ—Ç Shape
class Circle extends Shape {
  final double radius;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –ø–µ—Ä–µ–¥–∞—á–∏ —Ä–∞–¥–∏—É—Å–∞
  Circle(this.radius);

  // –†–µ–∞–ª–∏–∑–∞—Ü–∏—è –º–µ—Ç–æ–¥–∞ getArea() –¥–ª—è –∫—Ä—É–≥–∞
  @override
  double getArea() {
    return 3.14159 * radius * radius; // –§–æ—Ä–º—É–ª–∞ –ø–ª–æ—â–∞–¥–∏ –∫—Ä—É–≥–∞: œÄr¬≤
  }
}

// –ö–ª–∞—Å—Å Rectangle, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π –æ—Ç Shape
class Rectangle extends Shape {
  final double width;
  final double height;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –ø–µ—Ä–µ–¥–∞—á–∏ —à–∏—Ä–∏–Ω—ã –∏ –≤—ã—Å–æ—Ç—ã
  Rectangle(this.width, this.height);

  // –†–µ–∞–ª–∏–∑–∞—Ü–∏—è –º–µ—Ç–æ–¥–∞ getArea() –¥–ª—è –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞
  @override
  double getArea() {
    return width * height; // –§–æ—Ä–º—É–ª–∞ –ø–ª–æ—â–∞–¥–∏ –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞: —à–∏—Ä–∏–Ω–∞ * –≤—ã—Å–æ—Ç–∞
  }
}

void main() {
  // –ü—Ä–∏–º–µ—Ä –∫—Ä—É–≥–∞ —Å —Ä–∞–¥–∏—É—Å–æ–º 5
  Shape circle = Circle(5.0);
  print('–ü–ª–æ—â–∞–¥—å –∫—Ä—É–≥–∞: ${circle.getArea()}');

  // –ü—Ä–∏–º–µ—Ä –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞ —Å —à–∏—Ä–∏–Ω–æ–π 4 –∏ –≤—ã—Å–æ—Ç–æ–π 6
  Shape rectangle = Rectangle(4.0, 6.0);
  print('–ü–ª–æ—â–∞–¥—å –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞: ${rectangle.getArea()}');
}


–ó–∞–¥–∞—á–∞ 9
// –ë–∞–∑–æ–≤—ã–π –∫–ª–∞—Å—Å Animal
class Animal {
  // –ú–µ—Ç–æ–¥ move(), –∫–æ—Ç–æ—Ä—ã–π –±—É–¥–µ—Ç –ø–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω –≤ –ø–æ—Ç–æ–º–∫–∞—Ö
  void move() {
    print("–ñ–∏–≤–æ—Ç–Ω–æ–µ –¥–≤–∏–∂–µ—Ç—Å—è...");
  }
}

// –ö–ª–∞—Å—Å Fish, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π Animal
class Fish extends Animal {
  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ move()
  @override
  void move() {
    print("–†—ã–±–∞ –ø–ª–∞–≤–∞–µ—Ç...");
  }
}

// –ö–ª–∞—Å—Å Bird, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π Animal
class Bird extends Animal {
  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ move()
  @override
  void move() {
    print("–ü—Ç–∏—Ü–∞ –ª–µ—Ç–∞–µ—Ç...");
  }
}

// –ö–ª–∞—Å—Å Dog, –Ω–∞—Å–ª–µ–¥—É—é—â–∏–π Animal
class Dog extends Animal {
  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ move()
  @override
  void move() {
    print("–°–æ–±–∞–∫–∞ –±–µ–≥–∞–µ—Ç...");
  }
}

void main() {
  // –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç—ã –∫–ª–∞—Å—Å–æ–≤
  Animal genericAnimal = Animal();
  Fish fish = Fish();
  Bird bird = Bird();
  Dog dog = Dog();

  // –í—ã–∑—ã–≤–∞–µ–º –º–µ—Ç–æ–¥—ã move() –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –æ–±—ä–µ–∫—Ç–∞
  genericAnimal.move(); // –í—ã–≤–æ–¥: "–ñ–∏–≤–æ—Ç–Ω–æ–µ –¥–≤–∏–∂–µ—Ç—Å—è..."
  fish.move();          // –í—ã–≤–æ–¥: "–†—ã–±–∞ –ø–ª–∞–≤–∞–µ—Ç..."
  bird.move();          // –í—ã–≤–æ–¥: "–ü—Ç–∏—Ü–∞ –ª–µ—Ç–∞–µ—Ç..."
  dog.move();           // –í—ã–≤–æ–¥: "–°–æ–±–∞–∫–∞ –±–µ–≥–∞–µ—Ç..."
}


–ó–∞–¥–∞—á–∞ 10
// –ö–ª–∞—Å—Å Student
class Student {
  String name; // –ò–º—è —Å—Ç—É–¥–µ–Ω—Ç–∞
  String group; // –ì—Ä—É–ø–ø–∞ —Å—Ç—É–¥–µ–Ω—Ç–∞
  double grade; // –£—Å–ø–µ–≤–∞–µ–º–æ—Å—Ç—å/–æ—Ü–µ–Ω–∫–∞ —Å—Ç—É–¥–µ–Ω—Ç–∞

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  Student({required this.name, required this.group, required this.grade});

  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ toString –¥–ª—è —É–¥–æ–±–Ω–æ–≥–æ –≤—ã–≤–æ–¥–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —Å—Ç—É–¥–µ–Ω—Ç–µ
  @override
  String toString() {
    return 'Student{name: $name, group: $group, grade: $grade}';
  }
}

// –ö–ª–∞—Å—Å University
class University {
  List<Student> students = []; // –°–ø–∏—Å–æ–∫ —Å—Ç—É–¥–µ–Ω—Ç–æ–≤

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —Å—Ç—É–¥–µ–Ω—Ç–∞ –≤ —Å–ø–∏—Å–æ–∫
  void addStudent(Student student) {
    students.add(student);
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∏ —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ –ø–æ –∏–º–µ–Ω–∏
  void sortByName() {
    students.sort((a, b) => a.name.compareTo(b.name));
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–∏ —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ –ø–æ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–π —É—Å–ø–µ–≤–∞–µ–º–æ—Å—Ç–∏
  List<Student> filterByGrade(double minGrade) {
    return students.where((student) => student.grade >= minGrade).toList();
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –≤—Å–µ—Ö —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ (–¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏)
  void showAllStudents() {
    for (var student in students) {
      print(student);
    }
  }
}

void main() {
  // –°–æ–∑–¥–∞–µ–º —ç–∫–∑–µ–º–ø–ª—è—Ä —É–Ω–∏–≤–µ—Ä—Å–∏—Ç–µ—Ç–∞
  var university = University();

  // –î–æ–±–∞–≤–ª—è–µ–º —Å—Ç—É–¥–µ–Ω—Ç–æ–≤
  university.addStudent(Student(name: '–ò–≤–∞–Ω', group: 'A1', grade: 4.5));
  university.addStudent(Student(name: '–ú–∞—Ä–∏—è', group: 'B2', grade: 3.9));
  university.addStudent(Student(name: '–ê–ª–µ–∫—Å–µ–π', group: 'A1', grade: 4.8));
  university.addStudent(Student(name: '–°–æ—Ñ–∏—è', group: 'C3', grade: 5.0));
  university.addStudent(Student(name: '–î–º–∏—Ç—Ä–∏–π', group: 'B2', grade: 3.6));

  print('–í—Å–µ —Å—Ç—É–¥–µ–Ω—Ç—ã:');
  university.showAllStudents();

  // –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ –ø–æ –∏–º–µ–Ω–∏
  university.sortByName();
  print('\n–ü–æ—Å–ª–µ —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∏ –ø–æ –∏–º–µ–Ω–∏:');
  university.showAllStudents();

  // –§–∏–ª—å—Ç—Ä–∞—Ü–∏—è —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ –ø–æ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–π –æ—Ü–µ–Ω–∫–µ
  double minGrade = 4.0;
  print('\n–°—Ç—É–¥–µ–Ω—Ç—ã —Å —É—Å–ø–µ–≤–∞–µ–º–æ—Å—Ç—å—é –≤—ã—à–µ $minGrade:');
  var filteredStudents = university.filterByGrade(minGrade);
  filteredStudents.forEach(print);
}

–ó–∞–¥–∞—á–∞ 11
// –ö–ª–∞—Å—Å Product (—Ç–æ–≤–∞—Ä)
class Product {
  String name; // –ù–∞–∑–≤–∞–Ω–∏–µ —Ç–æ–≤–∞—Ä–∞
  double price; // –¶–µ–Ω–∞ —Ç–æ–≤–∞—Ä–∞
  int quantity; // –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–æ–≤–∞—Ä–∞ –Ω–∞ —Å–∫–ª–∞–¥–µ

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  Product({required this.name, required this.price, required this.quantity});

  // –ü–µ—Ä–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Ç–æ–¥–∞ toString –¥–ª—è —É–¥–æ–±–Ω–æ–≥–æ –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –ø—Ä–æ–¥—É–∫—Ç–µ
  @override
  String toString() {
    return 'Product{name: $name, price: $price, quantity: $quantity}';
  }
}

// –ö–ª–∞—Å—Å Store (–º–∞–≥–∞–∑–∏–Ω)
class Store {
  List<Product> products = []; // –°–ø–∏—Å–æ–∫ —Ç–æ–≤–∞—Ä–æ–≤ –≤ –º–∞–≥–∞–∑–∏–Ω–µ

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø—Ä–æ–¥—É–∫—Ç–∞
  void addProduct(Product product) {
    // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –µ—Å—Ç—å –ª–∏ —É–∂–µ —Ç–æ–≤–∞—Ä —Å —Ç–∞–∫–∏–º –∏–º–µ–Ω–µ–º
    var existingProduct = products.firstWhere(
        (p) => p.name.toLowerCase() == product.name.toLowerCase(),
        orElse: () => null);
    if (existingProduct != null) {
      // –ï—Å–ª–∏ —Ç–æ–≤–∞—Ä —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç, —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º –µ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ
      existingProduct.quantity += product.quantity;
      print('Quantity of "${product.name}" updated.');
    } else {
      // –ï—Å–ª–∏ —Ç–æ–≤–∞—Ä–∞ –Ω–µ—Ç, –¥–æ–±–∞–≤–ª—è–µ–º –Ω–æ–≤—ã–π –ø—Ä–æ–¥—É–∫—Ç
      products.add(product);
      print('Product "${product.name}" added.');
    }
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è –ø—Ä–æ–¥—É–∫—Ç–∞ –ø–æ –∏–º–µ–Ω–∏
  void removeProduct(String name) {
    var removed = products.removeWhere(
        (product) => product.name.toLowerCase() == name.toLowerCase());
    if (removed > 0) {
      print('Product "$name" removed.');
    } else {
      print('Product "$name" not found.');
    }
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –ø–æ–∏—Å–∫–∞ —Ç–æ–≤–∞—Ä–∞ –ø–æ –∏–º–µ–Ω–∏
  Product? findProductByName(String name) {
    return products.firstWhere(
        (product) => product.name.toLowerCase() == name.toLowerCase(),
        orElse: () => null);
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –≤—Å–µ—Ö –ø—Ä–æ–¥—É–∫—Ç–æ–≤ –≤ –º–∞–≥–∞–∑–∏–Ω–µ
  void showAllProducts() {
    if (products.isEmpty) {
      print('The store is empty.');
    } else {
      print('Products in the store:');
      for (var product in products) {
        print(product);
      }
    }
  }
}

// –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è
void main() {
  // –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç –º–∞–≥–∞–∑–∏–Ω–∞
  var store = Store();

  // –î–æ–±–∞–≤–ª—è–µ–º —Ç–æ–≤–∞—Ä—ã
  store.addProduct(Product(name: 'Apple', price: 1.2, quantity: 50));
  store.addProduct(Product(name: 'Banana', price: 0.8, quantity: 30));
  store.addProduct(Product(name: 'Orange', price: 1.5, quantity: 20));

  // –í—ã–≤–æ–¥–∏–º –≤—Å–µ —Ç–æ–≤–∞—Ä—ã
  store.showAllProducts();

  // –ü–æ–∏—Å–∫ —Ç–æ–≤–∞—Ä–∞
  var product = store.findProductByName('Banana');
  if (product != null) {
    print('Found product: $product');
  } else {
    print('Product not found.');
  }

  // –£–¥–∞–ª–µ–Ω–∏–µ —Ç–æ–≤–∞—Ä–∞
  store.removeProduct('Banana');

  // –í—ã–≤–æ–¥–∏–º –≤—Å–µ —Ç–æ–≤–∞—Ä—ã –ø–æ—Å–ª–µ —É–¥–∞–ª–µ–Ω–∏—è
  store.showAllProducts();

  // –î–æ–±–∞–≤–ª—è–µ–º –±–æ–ª—å—à–µ —è–±–ª–æ–∫
  store.addProduct(Product(name: 'Apple', price: 1.2, quantity: 25));

  // –í—ã–≤–æ–¥–∏–º –∏—Ç–æ–≥–æ–≤—ã–π —Å–ø–∏—Å–æ–∫ —Ç–æ–≤–∞—Ä–æ–≤
  store.showAllProducts();
}

–ó–∞–¥–∞—á–∞ 12
// –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å PaymentSystem
abstract class PaymentSystem {
  // –ú–µ—Ç–æ–¥ –¥–ª—è –æ–ø–ª–∞—Ç—ã
  void pay(double amount);

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≤–æ–∑–≤—Ä–∞—Ç–∞ —Å—Ä–µ–¥—Å—Ç–≤
  void refund(double amount);
}

// –ö–ª–∞—Å—Å CreditCard, —Ä–µ–∞–ª–∏–∑—É—é—â–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å PaymentSystem
class CreditCard implements PaymentSystem {
  String cardNumber;

  CreditCard(this.cardNumber);

  @override
  void pay(double amount) {
    print('–û–ø–ª–∞—Ç–∞ $amount —á–µ—Ä–µ–∑ –∫—Ä–µ–¥–∏—Ç–Ω—É—é –∫–∞—Ä—Ç—É $cardNumber –≤—ã–ø–æ–ª–Ω–µ–Ω–∞.');
  }

  @override
  void refund(double amount) {
    print('–í–æ–∑–≤—Ä–∞—Ç $amount –Ω–∞ –∫—Ä–µ–¥–∏—Ç–Ω—É—é –∫–∞—Ä—Ç—É $cardNumber –≤—ã–ø–æ–ª–Ω–µ–Ω.');
  }
}

// –ö–ª–∞—Å—Å PayPal, —Ä–µ–∞–ª–∏–∑—É—é—â–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å PaymentSystem
class PayPal implements PaymentSystem {
  String email;

  PayPal(this.email);

  @override
  void pay(double amount) {
    print('–û–ø–ª–∞—Ç–∞ $amount —á–µ—Ä–µ–∑ PayPal –∞–∫–∫–∞—É–Ω—Ç $email –≤—ã–ø–æ–ª–Ω–µ–Ω–∞.');
  }

  @override
  void refund(double amount) {
    print('–í–æ–∑–≤—Ä–∞—Ç $amount –Ω–∞ PayPal –∞–∫–∫–∞—É–Ω—Ç $email –≤—ã–ø–æ–ª–Ω–µ–Ω.');
  }
}

// –¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ —Ä–∞–±–æ—Ç—ã –∫–ª–∞—Å—Å–æ–≤
void main() {
  // –°–æ–∑–¥–∞–µ–º –ø–ª–∞—Ç–µ–∂–Ω—É—é —Å–∏—Å—Ç–µ–º—É CreditCard
  PaymentSystem creditCard = CreditCard('1234-5678-9012-3456');
  creditCard.pay(100.0);
  creditCard.refund(50.0);

  // –°–æ–∑–¥–∞–µ–º –ø–ª–∞—Ç–µ–∂–Ω—É—é —Å–∏—Å—Ç–µ–º—É PayPal
  PaymentSystem payPal = PayPal('user@example.com');
  payPal.pay(200.0);
  payPal.refund(75.0);
}


–ó–∞–¥–∞—á–∞ 13
// –ö–ª–∞—Å—Å UniqueID –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä–æ–≤
class UniqueID {
  // –°—Ç–∞—Ç–∏—á–µ—Å–∫–æ–µ –ø–æ–ª–µ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è —Ç–µ–∫—É—â–µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏—è —Å—á–µ—Ç—á–∏–∫–∞
  static int _counter = 0;

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —É–Ω–∏–∫–∞–ª—å–Ω–æ–≥–æ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä–∞
  static int generateID() {
    _counter++; // –£–≤–µ–ª–∏—á–µ–Ω–∏–µ —Å—á–µ—Ç—á–∏–∫–∞ –Ω–∞ 1
    return _counter; // –í–æ–∑–≤—Ä–∞—Ç –Ω–æ–≤–æ–≥–æ ID
  }
}

// –ü—Ä–∏–º–µ—Ä –∫–ª–∞—Å—Å–∞, –∫–æ—Ç–æ—Ä—ã–π –∏—Å–ø–æ–ª—å–∑—É–µ—Ç UniqueID
class MyClass {
  // ID –æ–±—ä–µ–∫—Ç–∞
  final int id;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä, –∫–æ—Ç–æ—Ä—ã–π –ø–æ–ª—É—á–∞–µ—Ç —É–Ω–∏–∫–∞–ª—å–Ω—ã–π ID
  MyClass() : id = UniqueID.generateID();

  @override
  String toString() => 'MyClass Object ID: $id';
}

void main() {
  // –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç—ã –∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–µ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä—ã
  MyClass obj1 = MyClass();
  MyClass obj2 = MyClass();
  MyClass obj3 = MyClass();

  print(obj1); // –í—ã–≤–æ–¥: MyClass Object ID: 1
  print(obj2); // –í—ã–≤–æ–¥: MyClass Object ID: 2
  print(obj3); // –í—ã–≤–æ–¥: MyClass Object ID: 3
}

–ó–∞–¥–∞—á–∞ 14
// –ö–ª–∞—Å—Å "–¢–æ—á–∫–∞" —Å –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º–∏ x –∏ y
class Point {
  double x;
  double y;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç —Ç–æ—á–∫–∏
  Point(this.x, this.y);

  @override
  String toString() => 'Point($x, $y)';
}

// –ö–ª–∞—Å—Å "–ü—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫", —Å–æ–¥–µ—Ä–∂–∞—â–∏–π –¥–≤–µ —Ç–æ—á–∫–∏
class Rectangle {
  Point topLeft; // –õ–µ–≤–∞—è –≤–µ—Ä—Ö–Ω—è—è —Ç–æ—á–∫–∞
  Point bottomRight; // –ü—Ä–∞–≤–∞—è –Ω–∏–∂–Ω—è—è —Ç–æ—á–∫–∞

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ —Ç–æ—á–µ–∫ –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞
  Rectangle(this.topLeft, this.bottomRight);

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≤—ã—á–∏—Å–ª–µ–Ω–∏—è –ø–ª–æ—â–∞–¥–∏ –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞
  double getArea() {
    // –í—ã—á–∏—Å–ª–µ–Ω–∏–µ —à–∏—Ä–∏–Ω—ã (–ø–æ x)
    double width = (bottomRight.x - topLeft.x).abs();
    // –í—ã—á–∏—Å–ª–µ–Ω–∏–µ –≤—ã—Å–æ—Ç—ã (–ø–æ y)
    double height = (topLeft.y - bottomRight.y).abs();

    // –ü–ª–æ—â–∞–¥—å –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞ = —à–∏—Ä–∏–Ω–∞ * –≤—ã—Å–æ—Ç–∞
    return width * height;
  }

  @override
  String toString() =>
      'Rectangle(TopLeft: $topLeft, BottomRight: $bottomRight)';
}

void main() {
  // –°–æ–∑–¥–∞–µ–º —Ç–æ—á–∫–∏
  Point topLeft = Point(1.0, 4.0);
  Point bottomRight = Point(5.0, 1.0);

  // –°–æ–∑–¥–∞–µ–º –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫ –∏–∑ —Ç–æ—á–µ–∫
  Rectangle rectangle = Rectangle(topLeft, bottomRight);

  // –í—ã–≤–æ–¥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–µ –∏ –µ–≥–æ –ø–ª–æ—â–∞–¥–∏
  print(rectangle); // –í—ã–≤–æ–¥–∏—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —Ç–æ—á–∫–∞—Ö
  print('Area of the rectangle: ${rectangle.getArea()}'); // –ü–ª–æ—â–∞–¥—å
}

–ó–∞–¥–∞—á–∞ 15

import 'dart:math';

// –ö–ª–∞—Å—Å –¥–ª—è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏—è –∫–æ–º–ø–ª–µ–∫—Å–Ω–æ–≥–æ —á–∏—Å–ª–∞
class ComplexNumber {
  double real; // –î–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–∞—è —á–∞—Å—Ç—å
  double imaginary; // –ú–Ω–∏–º–∞—è —á–∞—Å—Ç—å

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ –∫–æ–º–ø–ª–µ–∫—Å–Ω–æ–≥–æ —á–∏—Å–ª–∞
  ComplexNumber(this.real, this.imaginary);

  // –ú–µ—Ç–æ–¥ –¥–ª—è —Å–ª–æ–∂–µ–Ω–∏—è –¥–≤—É—Ö –∫–æ–º–ø–ª–µ–∫—Å–Ω—ã—Ö —á–∏—Å–µ–ª
  ComplexNumber add(ComplexNumber other) {
    return ComplexNumber(
      this.real + other.real,
      this.imaginary + other.imaginary,
    );
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –≤—ã—á–∏—Ç–∞–Ω–∏—è –¥–≤—É—Ö –∫–æ–º–ø–ª–µ–∫—Å–Ω—ã—Ö —á–∏—Å–µ–ª
  ComplexNumber subtract(ComplexNumber other) {
    return ComplexNumber(
      this.real - other.real,
      this.imaginary - other.imaginary,
    );
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è —É–º–Ω–æ–∂–µ–Ω–∏—è –¥–≤—É—Ö –∫–æ–º–ø–ª–µ–∫—Å–Ω—ã—Ö —á–∏—Å–µ–ª
  ComplexNumber multiply(ComplexNumber other) {
    double realPart = this.real * other.real - this.imaginary * other.imaginary;
    double imaginaryPart = this.real * other.imaginary + this.imaginary * other.real;
    return ComplexNumber(realPart, imaginaryPart);
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–µ–ª–µ–Ω–∏—è –¥–≤—É—Ö –∫–æ–º–ø–ª–µ–∫—Å–Ω—ã—Ö —á–∏—Å–µ–ª
  ComplexNumber divide(ComplexNumber other) {
    double denominator = other.real * other.real + other.imaginary * other.imaginary;
    if (denominator == 0) {
      throw ArgumentError("–î–µ–ª–µ–Ω–∏–µ –Ω–∞ –Ω–æ–ª—å –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ");
    }

    double realPart = (this.real * other.real + this.imaginary * other.imaginary) / denominator;
    double imaginaryPart = (this.imaginary * other.real - this.real * other.imaginary) / denominator;
    return ComplexNumber(realPart, imaginaryPart);
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ —Ç–µ–∫—Å—Ç–æ–≤–æ–≥–æ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏—è –∫–æ–º–ø–ª–µ–∫—Å–Ω–æ–≥–æ —á–∏—Å–ª–∞
  @override
  String toString() {
    if (imaginary >= 0) {
      return '${real} + ${imaginary}i';
    } else {
      return '${real} - ${imaginary.abs()}i';
    }
  }
}

void main() {
  // –ü—Ä–∏–º–µ—Ä—ã –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∫–ª–∞—Å—Å–∞ ComplexNumber

  // –°–æ–∑–¥–∞–µ–º –¥–≤–∞ –∫–æ–º–ø–ª–µ–∫—Å–Ω—ã—Ö —á–∏—Å–ª–∞
  ComplexNumber num1 = ComplexNumber(3, 4);
  ComplexNumber num2 = ComplexNumber(1, -2);

  print("–ß–∏—Å–ª–æ 1: $num1");
  print("–ß–∏—Å–ª–æ 2: $num2");

  // –°–ª–æ–∂–µ–Ω–∏–µ
  ComplexNumber sum = num1.add(num2);
  print("–°—É–º–º–∞: $sum");

  // –í—ã—á–∏—Ç–∞–Ω–∏–µ
  ComplexNumber diff = num1.subtract(num2);
  print("–†–∞–∑–Ω–æ—Å—Ç—å: $diff");

  // –£–º–Ω–æ–∂–µ–Ω–∏–µ
  ComplexNumber product = num1.multiply(num2);
  print("–ü—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ: $product");

  // –î–µ–ª–µ–Ω–∏–µ
  try {
    ComplexNumber quotient = num1.divide(num2);
    print("–ß–∞—Å—Ç–Ω–æ–µ: $quotient");
  } catch (e) {
    print(e);
  }
}

–ó–∞–¥–∞—á–∞ 16
class Matrix {
  // –î–∞–Ω–Ω—ã–µ –º–∞—Ç—Ä–∏—Ü—ã - –¥–≤—É–º–µ—Ä–Ω—ã–π —Å–ø–∏—Å–æ–∫
  final List<List<double>> data;

  // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä
  Matrix(this.data);

  // –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ä–∞–∑–º–µ—Ä–æ–≤ –º–∞—Ç—Ä–∏—Ü—ã
  int get rowCount => data.length; // –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç—Ä–æ–∫
  int get colCount => data.isNotEmpty ? data[0].length : 0; // –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç–æ–ª–±—Ü–æ–≤

  // –ü–µ—Ä–µ–≥—Ä—É–∑–∫–∞ –æ–ø–µ—Ä–∞—Ç–æ—Ä–∞ "+" –¥–ª—è —Å–ª–æ–∂–µ–Ω–∏—è –º–∞—Ç—Ä–∏—Ü
  Matrix operator +(Matrix other) {
    // –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —Å–æ–≤–ø–∞–¥–µ–Ω–∏–µ —Ä–∞–∑–º–µ—Ä–æ–≤ –º–∞—Ç—Ä–∏—Ü
    if (rowCount != other.rowCount || colCount != other.colCount) {
      throw ArgumentError('–ú–∞—Ç—Ä–∏—Ü—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –¥–ª—è —Å–ª–æ–∂–µ–Ω–∏—è.');
    }

    // –°–ª–æ–∂–µ–Ω–∏–µ –º–∞—Ç—Ä–∏—Ü
    List<List<double>> result = List.generate(rowCount, (i) =>
        List.generate(colCount, (j) => data[i][j] + other.data[i][j]));

    return Matrix(result);
  }

  // –ü–µ—Ä–µ–≥—Ä—É–∑–∫–∞ –æ–ø–µ—Ä–∞—Ç–æ—Ä–∞ "*" –¥–ª—è —É–º–Ω–æ–∂–µ–Ω–∏—è –º–∞—Ç—Ä–∏—Ü
  Matrix operator *(Matrix other) {
    // –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å —É–º–Ω–æ–∂–µ–Ω–∏—è (—Å—Ç–æ–ª–±—Ü—ã –ø–µ—Ä–≤–æ–π == —Å—Ç—Ä–æ–∫–∏ –≤—Ç–æ—Ä–æ–π)
    if (colCount != other.rowCount) {
      throw ArgumentError('–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç–æ–ª–±—Ü–æ–≤ –ø–µ—Ä–≤–æ–π –º–∞—Ç—Ä–∏—Ü—ã –¥–æ–ª–∂–Ω–æ —Å–æ–≤–ø–∞–¥–∞—Ç—å —Å –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ–º —Å—Ç—Ä–æ–∫ –≤—Ç–æ—Ä–æ–π.');
    }

    // –£–º–Ω–æ–∂–µ–Ω–∏–µ –º–∞—Ç—Ä–∏—Ü
    List<List<double>> result = List.generate(rowCount, (i) =>
        List.generate(other.colCount, (j) {
          double sum = 0;
          for (int k = 0; k < colCount; k++) {
            sum += data[i][k] * other.data[k][j];
          }
          return sum;
        }));

    return Matrix(result);
  }

  // –ú–µ—Ç–æ–¥ –¥–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ –≤—ã–≤–æ–¥–∞ –º–∞—Ç—Ä–∏—Ü—ã
  @override
  String toString() {
    return data.map((row) => row.join('\t')).join('\n');
  }
}

void main() {
  // –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è

  // –ú–∞—Ç—Ä–∏—Ü–∞ A (2x3)
  Matrix a = Matrix([
    [1, 2, 3],
    [4, 5, 6]
  ]);

  // –ú–∞—Ç—Ä–∏—Ü–∞ B (2x3) - –¥–ª—è —Å–ª–æ–∂–µ–Ω–∏—è —Å A
  Matrix b = Matrix([
    [7, 8, 9],
    [10, 11, 12]
  ]);

  // –ú–∞—Ç—Ä–∏—Ü–∞ C (3x2) - –¥–ª—è —É–º–Ω–æ–∂–µ–Ω–∏—è —Å A
  Matrix c = Matrix([
    [1, 2],
    [3, 4],
    [5, 6]
  ]);

  // –°–ª–æ–∂–µ–Ω–∏–µ A –∏ B
  print('–ú–∞—Ç—Ä–∏—Ü–∞ A:');
  print(a);
  print('\n–ú–∞—Ç—Ä–∏—Ü–∞ B:');
  print(b);
  print('\nA + B:');
  print(a + b);

  // –£–º–Ω–æ–∂–µ–Ω–∏–µ A –∏ C
  print('\n–ú–∞—Ç—Ä–∏—Ü–∞ C:');
  print(c);
  print('\nA * C:');
  print(a * c);
}

–ó–∞–¥–∞—á–∞ 17
import 'dart:math';

// –ö–ª–∞—Å—Å "–û—Ä—É–∂–∏–µ"
class Weapon {
  String name;
  int damage;

  Weapon(this.name, this.damage);

  @override
  String toString() => "$name (–£—Ä–æ–Ω: $damage)";
}

// –ö–ª–∞—Å—Å "–ò–≥—Ä–æ–∫"
class Player {
  String name;
  int health;
  Weapon? weapon;

  Player(this.name, this.health);

  // –ê—Ç–∞–∫–∞ –ø—Ä–æ—Ç–∏–≤–Ω–∏–∫–∞
  void attack(Enemy enemy) {
    if (weapon == null) {
      print("$name –ø—ã—Ç–∞–µ—Ç—Å—è –∞—Ç–∞–∫–æ–≤–∞—Ç—å, –Ω–æ —É –Ω–µ–≥–æ –Ω–µ—Ç –æ—Ä—É–∂–∏—è!");
    } else {
      print("$name –∞—Ç–∞–∫—É–µ—Ç ${enemy.name} —Å –ø–æ–º–æ—â—å—é ${weapon!.name}!");
      enemy.takeDamage(weapon!.damage);
    }
  }

  // –ü–æ–ª—É—á–µ–Ω–∏–µ —É—Ä–æ–Ω–∞
  void takeDamage(int damage) {
    health -= damage;
    print("$name –ø–æ–ª—É—á–∏–ª $damage —É—Ä–æ–Ω–∞. –û—Å—Ç–∞–ª–æ—Å—å –∑–¥–æ—Ä–æ–≤—å—è: $health");
    if (health <= 0) {
      print("$name –ø–æ–≥–∏–±–∞–µ—Ç!");
    }
  }

  // –ü–æ–¥–Ω—è—Ç–∏–µ –æ—Ä—É–∂–∏—è
  void equipWeapon(Weapon newWeapon) {
    weapon = newWeapon;
    print("$name –ø–æ–¥–Ω–∏–º–∞–µ—Ç –æ—Ä—É–∂–∏–µ: ${weapon!.name}");
  }

  bool isAlive() => health > 0;
}

// –ö–ª–∞—Å—Å "–í—Ä–∞–≥"
class Enemy {
  String name;
  int health;
  int attackPower;

  Enemy(this.name, this.health, this.attackPower);

  // –ê—Ç–∞–∫–∞ –∏–≥—Ä–æ–∫–∞
  void attack(Player player) {
    print("$name –∞—Ç–∞–∫—É–µ—Ç ${player.name}!");
    player.takeDamage(attackPower);
  }

  // –ü–æ–ª—É—á–µ–Ω–∏–µ —É—Ä–æ–Ω–∞
  void takeDamage(int damage) {
    health -= damage;
    print("$name –ø–æ–ª—É—á–∏–ª $damage —É—Ä–æ–Ω–∞. –û—Å—Ç–∞–ª–æ—Å—å –∑–¥–æ—Ä–æ–≤—å—è: $health");
    if (health <= 0) {
      print("$name –±—ã–ª –ø–æ–±–µ–∂–¥–µ–Ω!");
    }
  }

  bool isAlive() => health > 0;
}

// –û—Å–Ω–æ–≤–Ω–∞—è –∏–≥—Ä–∞
void main() {
  // –°–æ–∑–¥–∞–µ–º –æ—Ä—É–∂–∏–µ
  Weapon sword = Weapon("–ú–µ—á", 25);
  Weapon axe = Weapon("–¢–æ–ø–æ—Ä", 30);

  // –°–æ–∑–¥–∞–µ–º –∏–≥—Ä–æ–∫–∞
  Player player = Player("–ì–µ—Ä–æ–π", 100);

  // –°–æ–∑–¥–∞–µ–º –≤—Ä–∞–≥–∞
  Enemy enemy = Enemy("–ó–ª–æ–±–Ω—ã–π –æ—Ä–∫", 80, 15);

  // –ò–≥—Ä–∞
  print("–ò–≥—Ä–∞ –Ω–∞—á–∞–ª–∞—Å—å!");

  // –í—ã–≤–æ–¥ –Ω–∞—á–∞–ª—å–Ω–æ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è
  print("${player.name} –≤—Å—Ç—Ä–µ—á–∞–µ—Ç ${enemy.name}.");

  // –ò–≥—Ä–æ–∫ –ø–æ–¥–Ω–∏–º–∞–µ—Ç –æ—Ä—É–∂–∏–µ
  player.equipWeapon(sword);

  // –ë–æ–π
  while (player.isAlive() && enemy.isAlive()) {
    // –ò–≥—Ä–æ–∫ –∞—Ç–∞–∫—É–µ—Ç –≤—Ä–∞–≥–∞
    player.attack(enemy);

    // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∂–∏–≤ –ª–∏ –≤—Ä–∞–≥
    if (!enemy.isAlive()) break;

    // –í—Ä–∞–≥ –∞—Ç–∞–∫—É–µ—Ç –∏–≥—Ä–æ–∫–∞
    enemy.attack(player);

    // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∂–∏–≤ –ª–∏ –∏–≥—Ä–æ–∫
    if (!player.isAlive()) break;
  }

  // –†–µ–∑—É–ª—å—Ç–∞—Ç –∏–≥—Ä—ã
  if (player.isAlive()) {
    print("–ò–≥—Ä–æ–∫ –ø–æ–±–µ–¥–∏–ª!");
  } else {
    print("–ò–≥—Ä–æ–∫ –ø–æ–≥–∏–±. –ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞.");
  }
}


–ó–∞–¥–∞—á–∞ 18
// –ö–ª–∞—Å—Å "–ü—Ä–æ–¥—É–∫—Ç"
class Product {
  String name;
  double price;

  Product(this.name, this.price);

  @override
  String toString() => "$name (\$${price.toStringAsFixed(2)})";
}

// –ö–ª–∞—Å—Å "–ü–æ–∫—É–ø–∞—Ç–µ–ª—å"
class Customer {
  String name;
  List<Order> orderHistory = []; // –ò—Å—Ç–æ—Ä–∏—è –∑–∞–∫–∞–∑–æ–≤

  Customer(this.name);

  // –î–æ–±–∞–≤–ª—è–µ—Ç –∑–∞–∫–∞–∑ –≤ –∏—Å—Ç–æ—Ä–∏—é –∑–∞–∫–∞–∑–æ–≤
  void addOrder(Order order) {
    orderHistory.add(order);
    print("–ó–∞–∫–∞–∑ –¥–ª—è –∫–ª–∏–µ–Ω—Ç–∞ $name —É—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω!");
  }

  // –ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç –∏—Å—Ç–æ—Ä–∏—é –∑–∞–∫–∞–∑–æ–≤ –∏ –∏—Ö –æ–±—â—É—é —Å—Ç–æ–∏–º–æ—Å—Ç—å
  void viewOrderHistory() {
    print("\n–ò—Å—Ç–æ—Ä–∏—è –∑–∞–∫–∞–∑–æ–≤ –¥–ª—è –∫–ª–∏–µ–Ω—Ç–∞ $name:");
    if (orderHistory.isEmpty) {
      print("–ó–∞–∫–∞–∑–æ–≤ –Ω–µ—Ç.");
    } else {
      double totalSpent = 0.0;
      for (Order order in orderHistory) {
        print(order);
        totalSpent += order.getTotalPrice();
      }
      print("\n–û–±—â–∞—è —Å—É–º–º–∞ –∑–∞–∫–∞–∑–æ–≤: \$${totalSpent.toStringAsFixed(2)}");
    }
  }
}

// –ö–ª–∞—Å—Å "–ó–∞–∫–∞–∑"
class Order {
  List<Product> products = [];

  // –ú–µ—Ç–æ–¥ –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø—Ä–æ–¥—É–∫—Ç–∞ –≤ –∑–∞–∫–∞–∑
  void addProduct(Product product) {
    products.add(product);
    print("${product.name} –¥–æ–±–∞–≤–ª–µ–Ω –≤ –∑–∞–∫–∞–∑.");
  }

  // –ü–æ–¥—Å—á–µ—Ç –æ–±—â–µ–π —Å—Ç–æ–∏–º–æ—Å—Ç–∏ –∑–∞–∫–∞–∑–∞
  double getTotalPrice() {
    return products.fold(0.0, (sum, product) => sum + product.price);
  }

  @override
  String toString() {
    if (products.isEmpty) {
      return "–ó–∞–∫–∞–∑ –ø—É—Å—Ç.";
    }
    String productList = products.map((p) => p.toString()).join(", ");
    return "–ó–∞–∫–∞–∑: $productList. –û–±—â–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å: \$${getTotalPrice().toStringAsFixed(2)}";
  }
}

// –¢–æ—á–∫–∞ –≤—Ö–æ–¥–∞
void main() {
  // –°–æ–∑–¥–∞–Ω–∏–µ –ø—Ä–æ–¥—É–∫—Ç–æ–≤
  Product apple = Product("–Ø–±–ª–æ–∫–æ", 1.50);
  Product banana = Product("–ë–∞–Ω–∞–Ω", 0.80);
  Product orange = Product("–ê–ø–µ–ª—å—Å–∏–Ω", 1.20);

  // –°–æ–∑–¥–∞–Ω–∏–µ –ø–æ–∫—É–ø–∞—Ç–µ–ª—è
  Customer customer = Customer("–ò–≤–∞–Ω –ò–≤–∞–Ω–æ–≤");

  // –°–æ–∑–¥–∞–Ω–∏–µ –ø–µ—Ä–≤–æ–≥–æ –∑–∞–∫–∞–∑–∞
  Order order1 = Order();
  order1.addProduct(apple);
  order1.addProduct(banana);

  // –î–æ–±–∞–≤–ª—è–µ–º –ø–µ—Ä–≤—ã–π –∑–∞–∫–∞–∑ –≤ –∏—Å—Ç–æ—Ä–∏—é –ø–æ–∫—É–ø–∞—Ç–µ–ª—è
  customer.addOrder(order1);

  // –°–æ–∑–¥–∞–Ω–∏–µ –≤—Ç–æ—Ä–æ–≥–æ –∑–∞–∫–∞–∑–∞
  Order order2 = Order();
  order2.addProduct(orange);
  order2.addProduct(banana);
  order2.addProduct(apple);

  // –î–æ–±–∞–≤–ª—è–µ–º –≤—Ç–æ—Ä–æ–π –∑–∞–∫–∞–∑ –≤ –∏—Å—Ç–æ—Ä–∏—é –ø–æ–∫—É–ø–∞—Ç–µ–ª—è
  customer.addOrder(order2);

  // –ü—Ä–æ—Å–º–æ—Ç—Ä –∏—Å—Ç–æ—Ä–∏–∏ –∑–∞–∫–∞–∑–æ–≤ –ø–æ–∫—É–ø–∞—Ç–µ–ª—è
  customer.viewOrderHistory();
}

–ó–∞–¥–∞—á–∞ 19
// –ë–∞–∑–æ–≤—ã–π –∫–ª–∞—Å—Å "–£—Å—Ç—Ä–æ–π—Å—Ç–≤–æ"
class Device {
  String brand;

  Device(this.brand); // –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –¥–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –±—Ä–µ–Ω–¥–∞

  void turnOn() {
    print("$brand –≤–∫–ª—é—á–µ–Ω.");
  }

  void turnOff() {
    print("$brand –≤—ã–∫–ª—é—á–µ–Ω.");
  }
}

// –ö–ª–∞—Å—Å "–°–º–∞—Ä—Ç—Ñ–æ–Ω", —É–Ω–∞—Å–ª–µ–¥–æ–≤–∞–Ω–Ω—ã–π –æ—Ç Device
class Smartphone extends Device {
  Smartphone(String brand) : super(brand); // –ü–µ—Ä–µ–¥–∞—á–∞ –±—Ä–µ–Ω–¥–∞ –≤ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–∏–π –∫–ª–∞—Å—Å

  // –£–Ω–∏–∫–∞–ª—å–Ω—ã–π –º–µ—Ç–æ–¥ "–°–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–æ–≥—Ä–∞—Ñ–∏—é"
  void takePhoto() {
    print("$brand –¥–µ–ª–∞–µ—Ç —Ñ–æ—Ç–æ–≥—Ä–∞—Ñ–∏—é...");
  }
}

// –ö–ª–∞—Å—Å "–ù–æ—É—Ç–±—É–∫", —É–Ω–∞—Å–ª–µ–¥–æ–≤–∞–Ω–Ω—ã–π –æ—Ç Device
class Laptop extends Device {
  Laptop(String brand) : super(brand); // –ü–µ—Ä–µ–¥–∞—á–∞ –±—Ä–µ–Ω–¥–∞ –≤ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–∏–π –∫–ª–∞—Å—Å

  // –£–Ω–∏–∫–∞–ª—å–Ω—ã–π –º–µ—Ç–æ–¥ "–ó–∞–ø—É—Å—Ç–∏—Ç—å –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ"
  void runApplication(String appName) {
    print("$brand –∑–∞–ø—É—Å–∫–∞–µ—Ç –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ $appName...");
  }
}

// –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏–∏ —Ä–∞–±–æ—Ç—ã
void main() {
  // –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç Smartphone
  Smartphone smartphone = Smartphone("Samsung");
  smartphone.turnOn(); // –ò—Å–ø–æ–ª—å–∑—É–µ–º –º–µ—Ç–æ–¥ –∏–∑ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–æ–≥–æ –∫–ª–∞—Å—Å–∞
  smartphone.takePhoto(); // –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–π –º–µ—Ç–æ–¥ —Å–º–∞—Ä—Ç—Ñ–æ–Ω–∞
  smartphone.turnOff(); // –ò—Å–ø–æ–ª—å–∑—É–µ–º –º–µ—Ç–æ–¥ –∏–∑ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–æ–≥–æ –∫–ª–∞—Å—Å–∞

  print(""); // –ü—É—Å—Ç–∞—è —Å—Ç—Ä–æ–∫–∞ –¥–ª—è —Ä–∞–∑–¥–µ–ª–∏—Ç–µ–ª—è

  // –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç Laptop
  Laptop laptop = Laptop("Dell");
  laptop.turnOn(); // –ò—Å–ø–æ–ª—å–∑—É–µ–º –º–µ—Ç–æ–¥ –∏–∑ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–æ–≥–æ –∫–ª–∞—Å—Å–∞
  laptop.runApplication("Visual Studio Code"); // –£–Ω–∏–∫–∞–ª—å–Ω—ã–π –º–µ—Ç–æ–¥ –Ω–æ—É—Ç–±—É–∫–∞
  laptop.turnOff(); // –ò—Å–ø–æ–ª—å–∑—É–µ–º –º–µ—Ç–æ–¥ –∏–∑ —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–æ–≥–æ –∫–ª–∞—Å—Å–∞
}

–ó–∞–¥–∞—á–∞ 20
import 'dart:io';

// –ö–ª–∞—Å—Å, –æ–ø–∏—Å—ã–≤–∞—é—â–∏–π –∏–≥—Ä–æ–∫–∞
class Player {
  final String name; // –ò–º—è –∏–≥—Ä–æ–∫–∞
  final String symbol; // –°–∏–º–≤–æ–ª –∏–≥—Ä–æ–∫–∞: 'X' –∏–ª–∏ 'O'

  Player(this.name, this.symbol);
}

// –ö–ª–∞—Å—Å, –∫–æ—Ç–æ—Ä—ã–π —É–ø—Ä–∞–≤–ª—è–µ—Ç –∏–≥—Ä–æ–π "–ö—Ä–µ—Å—Ç–∏–∫–∏-–Ω–æ–ª–∏–∫–∏"
class Game {
  List<List<String>> board; // –ò–≥—Ä–æ–≤–æ–µ –ø–æ–ª–µ
  Player player1; // –ü–µ—Ä–≤—ã–π –∏–≥—Ä–æ–∫
  Player player2; // –í—Ç–æ—Ä–æ–π –∏–≥—Ä–æ–∫
  Player? currentPlayer; // –¢–µ–∫—É—â–∏–π –∏–≥—Ä–æ–∫

  Game(this.player1, this.player2)
      : board = List.generate(3, (_) => List.generate(3, (_) => ' ')), // –°–æ–∑–¥–∞–µ–º –ø—É—Å—Ç–æ–µ –ø–æ–ª–µ 3x3
        currentPlayer = null;

  // –í—ã–≤–æ–¥ –∏–≥—Ä–æ–≤–æ–≥–æ –ø–æ–ª—è –Ω–∞ —ç–∫—Ä–∞–Ω
  void displayBoard() {
    print('\n');
    for (var row in board) {
      print(row.map((cell) => cell.isEmpty ? ' ' : cell).join(' | '));
      print('-' * 9);
    }
  }

  // –°–º–µ–Ω–∞ —Ç–µ–∫—É—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞
  void switchPlayer() {
    currentPlayer = (currentPlayer == player1) ? player2 : player1;
  }

  // –•–æ–¥ –∏–≥—Ä–æ–∫–∞
  bool makeMove(int row, int col) {
    if (row < 0 || row >= 3 || col < 0 || col >= 3 || board[row][col] != ' ') {
      print("–ù–µ–≤–µ—Ä–Ω—ã–π —Ö–æ–¥. –Ø—á–µ–π–∫–∞ —É–∂–µ –∑–∞–Ω—è—Ç–∞ –∏–ª–∏ –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç.");
      return false;
    }

    board[row][col] = currentPlayer!.symbol; // –ó–∞–ø–∏—Å—ã–≤–∞–µ–º —Å–∏–º–≤–æ–ª –∏–≥—Ä–æ–∫–∞ –≤ —è—á–µ–π–∫—É
    return true;
  }

  // –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–±–µ–¥—ã
  Player? checkWinner() {
    // –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ç—Ä–æ–∫
    for (var row in board) {
      if (row.every((cell) => cell == currentPlayer!.symbol)) {
        return currentPlayer;
      }
    }

    // –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ç–æ–ª–±—Ü–æ–≤
    for (int col = 0; col < 3; col++) {
      if (board[0][col] == currentPlayer!.symbol &&
          board[1][col] == currentPlayer!.symbol &&
          board[2][col] == currentPlayer!.symbol) {
        return currentPlayer;
      }
    }

    // –ü—Ä–æ–≤–µ—Ä–∫–∞ –¥–∏–∞–≥–æ–Ω–∞–ª–µ–π
    if (board[0][0] == currentPlayer!.symbol &&
        board[1][1] == currentPlayer!.symbol &&
        board[2][2] == currentPlayer!.symbol) {
      return currentPlayer;
    }

    if (board[0][2] == currentPlayer!.symbol &&
        board[1][1] == currentPlayer!.symbol &&
        board[2][0] == currentPlayer!.symbol) {
      return currentPlayer;
    }

    return null; // –ù–µ—Ç –ø–æ–±–µ–¥–∏—Ç–µ–ª—è
  }

  // –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –Ω–∏—á—å—é
  bool isDraw() {
    for (var row in board) {
      if (row.contains(' ')) {
        return false; // –ï—Å–ª–∏ –µ—Å—Ç—å –ø—É—Å—Ç–∞—è —è—á–µ–π–∫–∞, —Ç–æ –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º –∏–≥—Ä—É
      }
    }
    return true; // –í—Å–µ —è—á–µ–π–∫–∏ –∑–∞–ø–æ–ª–Ω–µ–Ω—ã
  }

  // –ó–∞–ø—É—Å–∫ –∏–≥—Ä—ã
  void start() {
    currentPlayer = player1; // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–µ—Ä–≤–æ–≥–æ –∏–≥—Ä–æ–∫–∞
    while (true) {
      displayBoard();
      print('${currentPlayer!.name} (${currentPlayer!.symbol}), –≤–∞—à —Ö–æ–¥!');
      print('–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä —Å—Ç—Ä–æ–∫–∏ (0-2):');
      int row = int.parse(stdin.readLineSync()!);
      print('–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä —Å—Ç–æ–ª–±—Ü–∞ (0-2):');
      int col = int.parse(stdin.readLineSync()!);

      if (makeMove(row, col)) {
        // –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø–æ–±–µ–¥—É –ø–æ—Å–ª–µ —É—Å–ø–µ—à–Ω–æ–≥–æ —Ö–æ–¥–∞
        Player? winner = checkWinner();
        if (winner != null) {
          displayBoard();
          print('üéâ –ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º, ${winner.name} (${winner.symbol}) –ø–æ–±–µ–¥–∏–ª!');
          break;
        } else if (isDraw()) {
          displayBoard();
          print('–ò–≥—Ä–∞ –∑–∞–∫–æ–Ω—á–∏–ª–∞—Å—å –Ω–∏—á—å–µ–π!');
          break;
        }

        // –°–º–µ–Ω–∞ –∏–≥—Ä–æ–∫–∞
        switchPlayer();
      }
    }
  }
}

void main() {
  // –°–æ–∑–¥–∞–µ–º –∏–≥—Ä–æ–∫–æ–≤
  print('–í–≤–µ–¥–∏—Ç–µ –∏–º—è –ø–µ—Ä–≤–æ–≥–æ –∏–≥—Ä–æ–∫–∞ (–∏–≥—Ä–∞–µ—Ç X):');
  String player1Name = stdin.readLineSync()!;
  print('–í–≤–µ–¥–∏—Ç–µ –∏–º—è –≤—Ç–æ—Ä–æ–≥–æ –∏–≥—Ä–æ–∫–∞ (–∏–≥—Ä–∞–µ—Ç O):');
  String player2Name = stdin.readLineSync()!;

  Player player1 = Player(player1Name, 'X');
  Player player2 = Player(player2Name, 'O');

  // –°–æ–∑–¥–∞–µ–º –∏–≥—Ä—É –∏ –∑–∞–ø—É—Å–∫–∞–µ–º –µ—ë
  Game game = Game(player1, player2);
  game.start();
}








